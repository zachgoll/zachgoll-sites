{
  "name": "zachgoll",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "COMPOSE_PROFILES=all docker-compose up",
    "dev:env": "NX_DATABASE_URL=postgresql://pguser:pguser@localhost:5432/personal-site",
    "dev:services": "COMPOSE_PROFILES=services docker-compose up",
    "dev:apps": "yarn dev:env nx run-many --target serve --projects=personal-site,design-system --parallel --host 0.0.0.0 --nx-bail=true",
    "dev:site": "yarn dev:env nx run personal-site:serve --host 0.0.0.0",
    "dev:design": "yarn dev:env nx run design-system:storybook --host 0.0.0.0",
    "build": "yarn nx build personal-site",
    "dev:prisma:generate": "yarn dev:env prisma generate",
    "dev:prisma:push": "yarn dev:env prisma db push",
    "dev:prisma:migrate": "yarn dev:env prisma migrate dev",
    "dev:prisma:create": "yarn dev:env prisma migrate dev --create-only",
    "dev:prisma:reset": "yarn dev:env prisma migrate reset",
    "dev:prisma:seed": "yarn dev:env prisma db seed",
    "dev:prisma:studio": "yarn dev:env prisma prisma studio",
    "prisma:deploy": "yarn dev:env prisma migrate deploy",
    "dev:docker:reset": "docker-compose down -v --rmi all --remove-orphans && docker system prune --all --volumes && docker-compose build",
    "aws:deploy": "./node_modules/.bin/cdk deploy"
  },
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@nrwl/next": "14.4.0-beta.2",
    "@prisma/client": "^3.5.0",
    "autoprefixer": "^10.4.0",
    "aws-cdk": "^2.29.1",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "core-js": "^3.6.5",
    "fast-csv": "^4.3.6",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "markdown-it": "^13.0.1",
    "next": "12.1.6",
    "next-mdx-remote": "^4.0.3",
    "next-remote-watch": "^1.0.0",
    "postcss": "^8.3.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-query": "^3.39.1",
    "regenerator-runtime": "0.13.9",
    "rehype-highlight": "^5.0.2",
    "remark-html": "^15.0.1",
    "replace-in-file": "^6.3.5",
    "tailwindcss": "^3.0.22",
    "tslib": "^2.0.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
