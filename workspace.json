{
  "version": 2,
  "projects": {
    "design-system": {
      "root": "libs/design-system",
      "sourceRoot": "libs/design-system/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/design-system/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/design-system"],
          "options": {
            "jestConfig": "libs/design-system/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/design-system/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/design-system",
            "config": {
              "configFolder": "libs/design-system/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "personal-site": {
      "root": "apps/personal-site",
      "sourceRoot": "apps/personal-site",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/personal-site",
            "outputPath": "dist/apps/personal-site",
            "assets": ["apps/personal-site/images", "apps/personal-site/posts"]
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "personal-site:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "personal-site:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "personal-site:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/personal-site"],
          "options": {
            "jestConfig": "apps/personal-site/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/personal-site/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "personal-site-e2e": {
      "root": "apps/personal-site-e2e",
      "sourceRoot": "apps/personal-site-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/personal-site-e2e/cypress.json",
            "devServerTarget": "personal-site:serve",
            "tsConfig": "apps/personal-site-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "personal-site:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/personal-site-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
